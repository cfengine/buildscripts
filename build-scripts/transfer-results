#!/bin/sh

. $(dirname "$0")/functions
. version

set -x

BUILDMACHINE="$1"

mkdir -p $BASEDIR/../../../output/${SCHEDULER}/${BUILD_NUMBER}
rsync -avr --delete "$BUILDMACHINE:build/output/*" \
    $BASEDIR/../../../output/${SCHEDULER}/${BUILD_NUMBER} \
    >/tmp/rsync.log

ssh "$BUILDMACHINE" "rm -rf build/output"
